    <arg name="world_name" value="$(find roboskel_ros_resources)/gazebo/worlds/tidy_small_hospital_room.world" />
    <arg name="robot_x" value="-1.5" />
    <arg name="robot_y" value="-2.0" />
    <arg name="robot_yaw" value="1.5" />
  </include>

</launch>
