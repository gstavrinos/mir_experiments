<?xml version="1.0" encoding="UTF-8"?>
<launch>

    <node pkg="octomap_server" type="octomap_tracking_server_node" name="octomap_tracking_server" args="$(find mir_experiments)/maps/tidy_small_depth_camera.bt" output="screen">
		<param name="resolution" value="0.05" />
		
        <param name="frame_id" type="string" value="odom_comb" />
        <param name="base_frame_id" type="string" value="odom_comb" />
        <param name="track_changes" type="boolean" value="true" />
        <param name="change_id_frame" type="string" value="odom_comb" />
        <param name="topic_changes" value="/octomap_tracking_server/changeset" />
        <param name="filter_ground" type="bool" value="true" />

        <param name="min_change_pub" value="0" />
		<!-- data source to integrate (PointCloud2) -->
        <remap from="cloud_in" to="/camera/depth/points" />
		<remap from="projected_map" to="projected_map2" />
	
    </node>


</launch>

